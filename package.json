    ],
    "nodes": [
      "dist/nodes/Viridem/Viridem.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "*",
    "eslint": "*",
    "eslint-plugin-n8n-nodes-base": "*",
    "gulp": "*",
    "prettier": "*",
    "typescript": "*",
    "n8n": "*"
